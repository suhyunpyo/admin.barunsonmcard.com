                                                        <div class="input-daterange data" style="display:inline;">
                                                            <input type="text" id="WeddingDate" asp-for="invitation_Detail.WeddingDate" class="form-control bg-light border-0 small" value="@weddingDate.ToString("yyyy-MM-dd")"
                                                                   match="@(matchName(prodItems, "WeddingDate"))" style="width:40%; display:inline;" readonly="readonly" />
