                                            weddingDate = DateTime.Parse(Model.invitation_Detail.WeddingDate);

                                        var timeTypeName = Model.invitation_Detail.Time_Type_Code;
                                        if (Model.invitation_Detail.Time_Type_Eng_YN == "Y")
                                            timeTypeName = Model.invitation_Detail.Time_Type_Code == "오후" ? "PM" : "AM";

                                        var weddingWeekName = Model.invitation_Detail.WeddingWeek;
                                        if (Model.invitation_Detail.WeddingWeek_Eng_YN == "Y")
                                        {
                                            switch (Model.invitation_Detail.WeddingWeek)
                                            {
                                                case "월":
                                                    weddingWeekName = "MON";
                                                    break;
                                                case "화":
                                                    weddingWeekName = "THE";
                                                    break;
                                                case "수":
                                                    weddingWeekName = "WED";
                                                    break;
                                                case "목":
                                                    weddingWeekName = "THU";
                                                    break;
                                                case "금":
                                                    weddingWeekName = "FRI";
                                                    break;
                                                case "토":
                                                    weddingWeekName = "SAT";
                                                    break;
                                                case "일":
                                                    weddingWeekName = "SUN";
                                                    break;
                                            }
                                        }

