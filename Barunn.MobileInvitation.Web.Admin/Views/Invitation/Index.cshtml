                                                    <tr>
                                                        <td class="table-td-background">사용여부</td>
                                                        <td>
                                                            <input type="radio" id="Flower_gift_Y" class="btn_use" name="invitation_Detail.Flower_gift_YN" value="Y" @(Model.invitation_Detail.Flower_gift_YN == "Y" ? "checked":"")>
                                                            <label for="Flower_gift_Y">사용함</label>
                                                            <input type="radio" id="Flower_gift_N" class="btn_unuse" name="invitation_Detail.Flower_gift_YN" value="N" @(Model.invitation_Detail.Flower_gift_YN != "Y" ? "checked":"")>
