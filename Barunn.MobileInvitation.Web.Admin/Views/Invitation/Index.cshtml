@model InvitationViewModel
@using System.Text.Json;
@using Barunn.MobileInvitation.Dac.Models.Barunson;
@{
    Layout = null;

    List<TB_ReservationWord> defaultInfo = (List<TB_ReservationWord>)ViewData["defaultInfos"];
    var prodItems = defaultInfo.Where(m => m.Product_Category_Codes.Contains(Model.ProductCategoryCode));
}
@functions{

    public string matchName(IEnumerable<TB_ReservationWord> prodItems, string id)
    {
        return prodItems.FirstOrDefault(m => m.MappingField == id)?.ReserveWord;
    }
}
