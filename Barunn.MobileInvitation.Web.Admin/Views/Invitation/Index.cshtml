                                                            <input type="hidden" id="WeddingYY" asp-for="invitation_Detail.WeddingYY" match="@(matchName(prodItems, "WeddingYY"))" />
                                                            <input type="hidden" id="WeddingMM" asp-for="invitation_Detail.WeddingMM" match="@(matchName(prodItems, "WeddingMM"))" />
                                                            <input type="hidden" id="WeddingDD" asp-for="invitation_Detail.WeddingDD" match="@(matchName(prodItems, "WeddingDD"))" />
                                                            <input type="hidden" id="WeddingWeek" asp-for="invitation_Detail.WeddingWeek" />
                                                            <input type="hidden" id="WeddingWeekName" value="@weddingWeekName" match="@(matchName(prodItems, "WeddingWeek"))" />
                                                        </div>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td class="table-td-background" style="width:30%">시간</td>
