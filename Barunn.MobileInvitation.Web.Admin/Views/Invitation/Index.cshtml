                                                        @for (var i = 0; i < timeTypeCodes.Count(); i++)
                                                        {
                                                            <input type="radio" id="Time_Type_Code" asp-for="invitation_Detail.Time_Type_Code" value="@timeTypeCodes[i]" />
                                                            <span>@timeTypeCodes[i]</span>
                                                        }

                                                        <input type="text" maxlength="2" id="WeddingHour" class="form-control form-control-sm input_match" match="@(matchName(prodItems, "WeddingHour"))" asp-for="invitation_Detail.WeddingHour" /><span>시</span>
                                                        <input type="text" maxlength="2" id="WeddingMin" class="form-control form-control-sm input_match" match="@(matchName(prodItems, "WeddingMin"))" asp-for="invitation_Detail.WeddingMin" /><span>분</span>

                                                        <input type="hidden" id="Time_Type_Name" match="@(matchName(prodItems, "Time_Type_Code"))" value="@timeTypeName" />
                                                        <input type="hidden" id="Time_Type_Code" asp-for="invitation_Detail.Time_Type_Code" />
                                                        <input type="hidden" id="WeddingHHmm" asp-for="invitation_Detail.WeddingHHmm" />
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td class="table-td-background" style="width:30%">요일</td>
                                                    <td>
                                                        @for (var i = 0; i < EngYn.Count(); i++)
                                                        {
                                                            <input type="radio" asp-for="invitation_Detail.WeddingWeek_Eng_YN" value="@EngYn.Keys.ElementAt(i)" />
                                                            <label class="ml-1 mr-1">@EngYn.Values.ElementAt(i)</label>
                                                        }
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td class="table-td-background" style="width:30%">시간구분</td>
                                                    <td>
                                                        @for (var i = 0; i < EngYn.Count(); i++)
                                                        {
                                                            <input type="radio" asp-for="invitation_Detail.Time_Type_Eng_YN" value="@EngYn.Keys.ElementAt(i)" />
                                                            <label class="ml-1 mr-1">@EngYn.Values.ElementAt(i)</label>
                                                        }
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>


                                <!-- 행사장 정보 -->
